
extend type Query {
    getItemBidsById(itemId: String): [Bid]
}
extend type Mutation {
    submitBid(bid: SubmitBidInput): MutationResponse
}

input SubmitBidInput {
    bidId: String
    userId: String
    itemId: String
    amount: Float
}


type Bid {
    bidId: String
    userId: String
    itemId: String
    amount: Float
}
